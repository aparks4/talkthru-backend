generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id     Int     @id @default(autoincrement())
  name   String @unique
  email  String @unique
  password String
  online Boolean @default(false)
}

